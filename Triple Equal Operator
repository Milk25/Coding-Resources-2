Equality ===
Equality with coercion ==

2 === 2 // true
true === true // true
undefined === null // false
"text" === "textssad" // false
"2" === 2 // false

[] === [] // false
{} === {} // false
(function(){}) === (function(){}) // false

var a = {} // undefined
var b = {} // undefined

a
{}
var b = a; // undefined
a === b // true

